<div id="clearance" class="sign-up container text-center">
  <h2 class="mb-3">Register</h2>
  <%= form_for @user, class: "form" do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="form-group">
      <%= form.submit "Register", class: "btn btn-primary btn-block mt-3" %>
    </div>

    <div class="form-group mt-3">
      <%= link_to "Sing in", sign_in_path, class: "btn btn-primary" %>
    </div>

  <% end %>
</div>
